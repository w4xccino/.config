  "homepage": "https://github.com/BubbatheVTOG/coc-discord-neovim",
  "repository": "github.com:BubbatheVTOG/coc-discord-neovim.git",
  "license": "MIT",
  "engines": {
    "coc": ">=0.0.70"
  },
  "devDependencies": {
    "@types/discord-rpc": "^3.0.2",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "coc.nvim": "^0.0.76",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-plugin-import": "^2.20.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "discord-rpc": "^3.1.0",
    "dotenv": "^8.2.0",
    "fp-ts": "^2.5.3"
