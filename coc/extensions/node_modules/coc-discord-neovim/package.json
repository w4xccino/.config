{
  "name": "coc-discord-neovim",
  "version": "0.0.2",
  "description": "A fork of coc-discord with added features such as file type art assets.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib/",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "eslint src/*",
    "prepack": "yarn clean && yarn lint && yarn build"
